<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mdgiitr.suyash.graph">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.mdgiitr.suyash.graph.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mdgiitr.suyash.graph.PieChartLanding" />
        <activity
            android:name="com.mdgiitr.suyash.graph.LineGraphLanding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.BarGraphLanding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.FreeCurveLanding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.LineGraphNew"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.LinePointNew"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.LineGraphDisplay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.BarPointNew"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.BarGraphNew"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.mdgiitr.suyash.graph.BarGraphDisplay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.mdgiitr.suyash.graph.PieChartNew" />
        <activity android:name="com.mdgiitr.suyash.graph.PieChartAddEntry" />
        <activity android:name="com.mdgiitr.suyash.graph.PieChartResult"/>

    </application>

</manifest>